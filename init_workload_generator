# Testing file path
# FILE_PATH /dev/sda1
FILE_PATH /media/choki98/sandisk/a

# TEST_INTERFAE_TYPE : 
# 	0 -> Character Device (Unit : byte) 
# 	1 -> Block Device (Unit : Sector)
TEST_INTERFACE_TYPE 0



# TEST_LENGTH_TYPE : 
# 	0 -> Time, 
# 	1 -> Number of Requests
TEST_LENGTH_TYPE 1

# TOTAL_TEST_REQUESTS : 
# 	if TEST_LENGTH_TYPE is time, 
# 	this value will be ignored
TOTAL_TEST_REQUESTS 20

# TOTAL_TEST_TIME : 
# 	Unit of value is 'ms' & 
# 		if TEST_LENGTH_TYPE is number of requests, 
# 	this value will be ignored
TOTAL_TEST_TIME 1000



# Unit of test address and size is 'byte'
MAX_ADDRESS 1000000000
MIN_ADDRESS 0

# Size of request
MAX_SIZE 10000000
MIN_SIZE 0

# Each operation type is selected by each weight
READ_W 100
WRITE_W 0

# Sequntiality is managed by weighs
SEQUENTIAL_W 0
NONSEQUENTIAL_W 100

# After requests as many as burstiness number are issued, request issuing is posed for pose time
BURSTINESS_NUMBER 10
# Unit of pose time is 'ms'
POSE_TIME 0

# ALIGNMENT : DIRECT_IO requires aligned start address & muliple of 512 size
# 	0 -> OFF, 
# 	1 -> ON
ALIGNMENT 1 

# ALIGNMENT_UNIT : 
# 	Unit is byte
ALIGNMENT_UNIT 512

# Make random number deterministically.
# 	1 -> Deterministic
# 	0 -> Non-deterministic
RANDOM_DETERMINISTIC 0

